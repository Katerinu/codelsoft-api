generator client {
  provider = "prisma-client-js"
  output   = "../prisma-bills-database/bills-client"
}

datasource db {
  provider = "mysql"
  url      = env("SQL_BILLS_DATABASE_URL")
}

model Placeholder {
  id    Int     @id @default(autoincrement())
  placeholderValue String
}